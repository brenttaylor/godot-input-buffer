; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Input buffer"
run/main_scene="res://demo/main_scene.tscn"
config/features=PackedStringArray("4.2")
run/max_fps=60
config/icon="res://icon.svg"

[autoload]

BufferedInput="*res://buffered_input/buffered_input.gd"
AudioBus="*res://demo/AudioBus/audio_bus.tscn"
GameManager="*res://demo/GameManager/game_manager.tscn"

[debug]

gdscript/warnings/untyped_declaration=2
gdscript/warnings/unsafe_property_access=2
gdscript/warnings/unsafe_method_access=2
gdscript/warnings/unsafe_cast=1
gdscript/warnings/unsafe_call_argument=2

[display]

window/size/viewport_width=360
window/size/viewport_height=270
window/size/window_width_override=1080
window/size/window_height_override=810
window/stretch/mode="viewport"
window/stretch/scale_mode="integer"
window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="Input buffer"

